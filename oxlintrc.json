{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "settings": {},
  "rules": {
    "@typescript-eslint/no-unused-vars": "error",
    "no-unused-vars": "off"
  },
  "plugins": ["typescript", "import", "react", "nextjs"],
  "categories": {
    "correctness": "error",
    "suspicious": "warn",
    "pedantic": "off",
    "style": "warn",
    "nursery": "off"
  }
}
